{
    "compilerOptions": {
        "target": "es6",
        "module": "esnext",
        "lib": ["es2017", "esnext.asynciterable"],
        "allowSyntheticDefaultImports": true,
        "jsx": "preserve",
        "moduleResolution": "node",
        "noEmit": true,
        "skipLibCheck": true,
        "resolveJsonModule": true,
        "esModuleInterop": true,
        "strict": false
    }
}
